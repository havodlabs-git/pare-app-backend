# Production Environment Variables for Cloud Run
# These will be set via Cloud Run environment variables

PORT=8080
NODE_ENV=production

# MongoDB URI (set via Cloud Run)
# MONGODB_URI=mongodb+srv://user:pass@cluster.mongodb.net/pare-app

# JWT Secret (set via Cloud Run)
# JWT_SECRET=your-production-jwt-secret

# JWT Expiration
JWT_EXPIRES_IN=7d

# CORS Origins (set via Cloud Run)
# CORS_ORIGIN=https://pare-app-483321.web.app

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
